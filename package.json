{
  "name": "discoverweeklydump",
  "productName": "Discover Weekly Dump",
  "version": "1.0.0",
  "description": "A script that uses the Spotify API to put all of the songs in the discover weekly into another playlist each week ",
  "main": "index.js",
  "scripts": {
    "start": "electron . && node index.js",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=icon.icns --prune=true --out=release-builds"
  },
  "author": "Keegan Lawley",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "figlet": "^1.2.1",
    "menubar": "^5.2.3",
    "node-cron": "^2.0.3",
    "puppeteer": "^1.11.0",
    "spotify-web-api-node": "^4.0.0"
  },
  "devDependencies": {
    "electron": "^4.0.0",
    "electron-packager": "^11.2.1"
  }
}
